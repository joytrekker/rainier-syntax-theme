  "tokenColors": [
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#9d79d8"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring.multi.python"],
      "settings": {
        "foreground": "#627d96"
      }
    },
    {
      "name": "Function",
      "scope": ["meta.function-call.generic","support.type.python"],
      "settings": {
        "foreground": "#509ddb"
      }
    },
    {
      "name": "Entity",
      "scope": ["entity", "entity.other"],
      "settings": {
        "foreground": "#37c2b6"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#dd518c"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#dd518c"
      }
    },
    {
